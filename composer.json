{
    "name": "slox-dwn/slox-dwn",
    "description": "Download Installed plugin in Shopware 6",
    "type": "shopware-platform-plugin",
    "version": "1.0.3",
    "license": "proprietary",
    "require": {
        "shopware/core": "6.6.7.*"
    },
    "extra": {
        "shopware-plugin-class": "SloxDwn\\SloxShopwarePluginDownload",
        "plugin-icon": "icon.png",
        "label": {
            "de-DE": "Plugin herunterladen",
            "en-GB": "Download Installed plugin"
          },
          "description": {
               "de-DE": "Download installiertes Plugin in Shopware 6",
               "en-GB": "Download Installed plugin in Shopware 6"
          }
    },
    "autoload": {
        "psr-4": {
            "SloxDwn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SloxDwn\\Tests\\": "tests/"
        }
    }
}
